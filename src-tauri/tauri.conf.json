{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:8000",
    "distDir": "../dist"
  },
  "package": {
    "productName": "aip",
    "version": "0.0.24"
  },
  "tauri": {
    "updater": {
      "active": true,
      "endpoints": [
        "https://wx.yryz3.com/tauri-updater/check/{{target}}/{{arch}}/{{current_version}}"
      ],
      "windows": {
        "installMode": "passive"
      },
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEY3OUQ4MjlDNzgyMjEzRTkKUldUcEV5SjRuSUtkOXlkTDhoQWM3WDIwSGNERlNuZFFUUjBSbUhZY29iKzRVZXpUeHdVSWRmRlMK"
    },
    "allowlist": {
      "all": false,
      "http": {
        "all": true,
        "request": true,
        "scope": [
          "http://**",
          "https://**",
          "ws://**",
          "wss://**"
        ]
      },
      "shell": {
        "all": true,
        "execute": true,
        "sidecar": true,
        "open": true,
        "scope": [
          {
            "name": "bin/ffmpeg",
            "args": true,
            "sidecar": true
          },
          {
            "name": "bin/ffprobe",
            "args": true,
            "sidecar": true
          },
          {
            "name": "bin/dssim",
            "args": true,
            "sidecar": true
          }
        ]
      },
      "fs": {
        "all": true,
        "scope": [
          "**"
        ]
      },
      "path": {
        "all": true
      },
      "dialog": {
        "all": true
      },
      "protocol": {
        "all": true,
        "asset": true,
        "assetScope": [
          "**"
        ]
      },
      "clipboard": {
        "all": true,
        "writeText": true,
        "readText": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.tauri.aip",
      "icon": [
        "icons.iconset/icon_16x16.png",
        "icons.iconset/icon_16x16@2x.png",
        "icons.iconset/icon_32x32.png",
        "icons.iconset/icon_32x32@2x.png",
        "icons.iconset/icon_128x128.png",
        "icons.iconset/icon_128x128@2x.png",
        "icons.iconset/icon_256x256.png",
        "icons.iconset/icon_256x256@2x.png",
        "icons.iconset/icon_512x512.png",
        "icons.iconset/icon_512x512@2x.png",
        "icons.iconset/icon.icns",
        "icons.iconset/icon.ico"
      ],
      "externalBin": [
        "bin/ffmpeg",
        "bin/ffprobe",
        "bin/dssim"
      ],
      "resources":[
        "resources/**"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "decorations": true,
        "fullscreen": false,
        "resizable": true,
        "title": "鹦鹉智绘-0.0.24",
        "center": true,
        "width": 1700,
        "height": 1000,
        "minWidth": 1700,
        "minHeight": 848
      }
    ]
  }
}
